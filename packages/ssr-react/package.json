{
  "name": "ssr",
  "version": "0.5.5",
  "private": true,
  "scripts": {
    "build": "npm run build:client && npm run build:server",
    "build-serve": "npm run build:client && npm run serve-dev",
    "build:client": "vite build --outDir dist/client0,5.5",
    "build:server": "vite build --ssr src/entry-server.jsx --outDir dist/server",
    "check": "tsc --noEmit --watch",
    "debug": "node --inspect-brk server",
    "dev": "vite --port 65533",
    "dev:ssr": "node --experimental-specifier-resolution=node --experimental-modules server",
    "generate": "vite build --outDir dist/static && npm run build:server && node prerender",
    "serve": "cross-env NODE_ENV=production node server",
    "serve-dev": "serve dist/client0,5.5 -l tcp://0.0.0.0:65535"
  },
  "dependencies": {
    "@types/lodash-es": "^4.17.6",
    "cookie-parser": "^1.4.6",
    "d3": "^7.4.4",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "file-saver": "^2.0.5",
    "lodash-es": "^4.17.21",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-dropzone": "^12.0.5",
    "react-json-viewer": "^3.0.1",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.3.0",
    "serve": "^13.0.2",
    "sno-mongo-ku": "^2.7.7",
    "snosay": "^0.1.27",
    "xlsx": "^0.18.5",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.5",
    "@vitejs/plugin-react": "^1.2.0",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "serve-static": "^1.14.1",
    "vite": "^*"
  }
}
